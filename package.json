    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "web-vitals": "^2.1.4"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.2.0",
    "@babel/core": "^7.12.16",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.2",
    "@mui/material": "^5.6.2",
    "async": "^3.2.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "create-app-react": "^1.0.6",
    "dayjs": "^1.10.3",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.17.1",
    "express-openid-connect": "^2.2.0",
    "http-proxy-middleware": "^2.0.6",
    "joi": "^17.6.0",
    "mic-recorder-to-mp3": "^2.2.2",
    "mongoose": "^5.11.11",
    "node-fetch": "^2.6.1",
    "npm": "^8.7.0",
    "react-dropzone": "^12.0.5",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.1",
